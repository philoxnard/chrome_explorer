{
    "name": "Phoxtrot",
    "version": "0.1.0",
    "description": "Front end for the Phoxtrot app",
    "permissions": ["storage", "declarativeContent", "scripting", "tabs", "notifications"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "templates/popup.html"
    },
    "web_accessible_resources": [{
        "resources": ["notification.png"]
    }],
    "manifest_version": 3
}